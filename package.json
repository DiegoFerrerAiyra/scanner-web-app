{
  "name": "apollo-ng",
  "version": "15.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4500 --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:production": "ng build --configuration=production",
    "build:analyzer": "ng build --configuration=production --stats-json",
    "analyzer": "npx webpack-bundle-analyzer dist/modak-live/stats.json",
    "build:development": "ng build --configuration=development",
    "build:stage": "ng build --configuration=stage",
    "build:storybook": "echo 1",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "pretty-quick",
    "enabled-husky": "husky install",
    "unused": "eslint --fix ./src/app",
    "coverage": "open-cli ./coverage/index.html && ng test --code-coverage",
    "test-report": "ng test --watch=false --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@fullcalendar/angular": "^6.0.3",
    "@fullcalendar/core": "^6.0.3",
    "@fullcalendar/daygrid": "^6.0.3",
    "@fullcalendar/interaction": "^6.0.3",
    "@fullcalendar/timegrid": "^6.0.3",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "chart.js": "^3.3.2",
    "crypto-js": "^4.1.1",
    "firebase": "^9.23.0",
    "jwt-decode": "^3.1.2",
    "luxon": "^3.3.0",
    "ngrx-store-localstorage": "^15.0.0",
    "ngx-clipboard": "^16.0.0",
    "ngx-cookie-service": "^15.0.0",
    "ngx-papaparse": "^8.0.0",
    "open-cli": "^7.2.0",
    "papaparse": "^5.4.1",
    "pretty-quick": "^3.1.3",
    "primeflex": "^3.2.1",
    "primeicons": "6.0.1",
    "primeng": "15.2.0",
    "prismjs": "1.9.0",
    "quill": "^1.3.7",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.0",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.0",
    "@angular/compiler-cli": "^15.2.0",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "~4.3.0",
    "@types/luxon": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
